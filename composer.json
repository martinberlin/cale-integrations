{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "beta",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "aws/aws-sdk-php-symfony": "^2.2",
        "davefx/phplot": "6.2.1",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "doctrine/orm": "^2.13",
        "endroid/qr-code": "^3.7",
        "google/apiclient": "^2.4",
        "gregwar/captcha-bundle": "2.1.4",
        "johngrogg/ics-parser": "^2",
        "mike182uk/paypal-ipn-listener": "^9.0",
        "payum/paypal-express-checkout-nvp": "^1.7",
        "payum/payum-bundle": "2.4.2",
        "php-http/guzzle7-adapter": "1.0.0",
        "php-http/message-factory": "^1.1",
        "sensio/framework-extra-bundle": "^5.4",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/http-client": "^4.4",
        "symfony/proxy-manager-bridge": "^4.4",
        "symfony/security-bundle": "^4.4",
        "symfony/swiftmailer-bundle": "^3.4@dev",
        "symfony/translation": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/yaml": "^4.4",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "symfony/debug-bundle": "^4.4",
        "symfony/monolog-bundle": "^3.0",
        "symfony/stopwatch": "^4.4",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "symfony": {
            "allow-contrib": true,
            "require": "^4.4"
        }
    }
}
